package model;

public class Gerente extends Pessoa{
	private String departamento;
	
	

	public Gerente() {
		super();
	}

	public Gerente(String departamento) {
		this.departamento = departamento;
	}
	
	public Gerente(String departamento, String nome, String cpf) {
		super(nome, cpf);
		this.departamento = departamento;
	}
	
	

}
