package exemplo12;

public class Analista {

	public void valeTransporte(double salario) {
		System.out.println("Vale Transporte padrão " + (salario * 0.06));
	}
	
	public void valeAlimentacao(double salario) {
		System.out.println("Vale Alimentação padrão " + (salario * 0.1));
	}
	
	
}
