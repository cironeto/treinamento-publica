package menus;

import javax.swing.JOptionPane;

import service.UsuarioService;

public class MenuCrudUsuarios {
	static int opcao = 0;
	
	public static void menu() {

		do {
			opcao = Integer
					.parseInt(JOptionPane.showInputDialog("*GERENCIAMENTO DE USUARIOS* \n" 
							+ "1 - Cadastrar \n" 
							+ "2 - Listar \n"
							+ "3 - Alterar \n" 
							+ "4 - remover \n" 
							+ "5 - Sair \n\n"));

			switch (opcao) {

			case 1:
				UsuarioService.
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			case 5:
				break;
			default:
				break;
			}
		} while (opcao != 5);
	}
}
